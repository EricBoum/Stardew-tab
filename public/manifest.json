{
  "manifest_version": 3,
  "name": "Stardew Tab",
  "description": "一个星露谷物语风格的标签页",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": "icon48.png"
  },
  "permissions": [
    "geolocation",
    "storage"
  ],
  "host_permissions": [
    "https://restapi.amap.com/*",
    "https://suggestion.baidu.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*.png", "assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
